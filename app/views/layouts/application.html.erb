<!DOCTYPE html>
<html>
  <head>
    <title>KvilleScheduler</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'kville' %>
    <%= stylesheet_pack_tag  'kville' %>
  </head>
  <div id="root"></div>

  <body>
    <%= yield %>
  </body>
</html>
